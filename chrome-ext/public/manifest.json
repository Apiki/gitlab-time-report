{
  "name": "GitLab Issue Time Logger",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Log your gitlab issue spent time",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "baurine",
  "options_page": "report.html",
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["time-logger.js"],
        "css": ["time-logger.css"]
      }
  ],
  "permissions": [
    "storage"
  ]
}
